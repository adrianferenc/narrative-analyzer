<%- include('../partials/header') %>
<!-- Masthead-->
<header class="masthead">
  <div class="container">
    <div class="masthead-subheading"><%=title%></div>
  </div>
</header>
<!-- Services-->
<section class="page-section" id="students">
  <div class="container">
    <form action="/students" method="post">
      <input
        class="form-control w-50"
        placeholder="Student's Name"
        type="text"
        name="name"
      />
      <select class="form-select w-25" aria-label="Grade" name="grade">
        <option selected value="Enter a grade">You must select a grade</option>
        <option value="A">A</option>
        <option value="A-">A-</option>
        <option value="B+">B+</option>
        <option value="B">B</option>
        <option value="B-">B-</option>
        <option value="C+">C+</option>
        <option value="C">C</option>
        <option value="C-">C-</option>
        <option value="D+">D+</option>
        <option value="D">D</option>
        <option value="D-">D-</option>
        <option value="F">F</option>
      </select>

      <button class="btn btn-lg btn-primary" type="submit">Add Student</button>
    </form>
  </div>
</section>
<!-- Footer-->
<%- include('../partials/footer') %>
